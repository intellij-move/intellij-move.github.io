<?xml version="1.0" encoding="UTF-8"?>
<keymap id="intellij_move_keymap">
  <layouts>
    <layout name="Windows" platform="PC"/>
    <layout name="macOS" platform="MAC"/>
    <layout name="Linux" platform="PC"/>
  </layouts>
  <actions>
    <action id="$DeclUsages">
      <description>Declaration or Usages</description>
      <shortcut layout="Windows">Ctrl+B</shortcut>
      <shortcut layout="macOS">⌘ B</shortcut>
      <shortcut layout="Linux">Ctrl+B</shortcut>
    </action>
    <action id="$SearchActions">
      <description>Search for Actions</description>
      <shortcut layout="Windows">Ctrl+Shift+A</shortcut>
      <shortcut layout="macOS">⌘ Shift A</shortcut>
      <shortcut layout="Linux">Ctrl+Shift+A</shortcut>
    </action>
  </actions>
</keymap>